START _> SINGLE_SELECTOR GUARD

GUARD _> SELECTOR_PROD_A
GUARD _> NON_EMPTY_WHITESPACES_PROD SELECTOR_PROD_B

SELECTOR_PROD_A _> COMMA WHITESPACES_PROD SINGLE_SELECTOR GUARD
SELECTOR_PROD_A _> GT WHITESPACES_PROD SINGLE_SELECTOR GUARD
SELECTOR_PROD_A _> PLUS WHITESPACES_PROD SINGLE_SELECTOR GUARD
SELECTOR_PROD_A _> NOTHING

SELECTOR_PROD_B _> COMMA WHITESPACES_PROD SINGLE_SELECTOR GUARD
SELECTOR_PROD_B _> GT WHITESPACES_PROD SINGLE_SELECTOR GUARD
SELECTOR_PROD_B _> PLUS WHITESPACES_PROD SINGLE_SELECTOR GUARD
SELECTOR_PROD_B _> SINGLE_SELECTOR GUARD
SELECTOR_PROD_B _> NOTHING

WHITESPACES_PROD _> NON_EMPTY_WHITESPACES_PROD
WHITESPACES_PROD _> NOTHING

NON_EMPTY_WHITESPACES_PROD _> WHITESPACES
NON_EMPTY_WHITESPACES_PROD _> TAB
NON_EMPTY_WHITESPACES_PROD _> SPACE

SINGLE_SELECTOR _> HASH CLASS
SINGLE_SELECTOR _> COLON CLASS
SINGLE_SELECTOR _> CLASS SINGLE_SELECTOR_1
SINGLE_SELECTOR _> DOT CLASS SINGLE_SELECTOR_1
SINGLE_SELECTOR_1 _> DOT CLASS SINGLE_SELECTOR_1
SINGLE_SELECTOR_1 _> HASH CLASS
SINGLE_SELECTOR_1 _> COLON CLASS
SINGLE_SELECTOR_1 _> NOTHING

CLASS _> STAR
CLASS _> CLASS_1
CLASS_1 _> HYPHEN CLASS_1
CLASS_1 _> CLASS_2
CLASS_2 _> ALPHA CLASS_2
CLASS_2 _> NUMERIC CLASS_2
CLASS_2 _> HYPHEN CLASS_2
CLASS_2 _> NOTHING
